{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Auth0",
  "colors": {
    "primary": "#5001FF",
    "light": "#5001FF",
    "dark": "#341773"
  },
  "favicon": "/favicon.svg",
  "fonts": {
    "heading": {
      "family": "Aeonik",
      "source": "https://cdn.auth0.com/website/cic-homepage/fonts/Aeonik-Medium.woff2",
      "format": "woff2"
    },
    "body": {
      "family": "Aeonik",
      "source": "https://cdn.auth0.com/website/cic-homepage/fonts/Aeonik-Regular.woff2",
      "format": "woff2"
    }
  },
  "contextual": {
    "options": ["copy", "view", "chatgpt", "claude"]
  },
  "appearance": {
    "default": "dark"
  },
  "background": {
    "decoration": "gradient"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Docs",
        "groups": [
          {
            "group": "Auth for GenAI",
            "pages": [
              "intro/overview",
              {
                "group": "User Authentication",
                "pages": [
                  "intro/user-authentication",
                  "intro/call-your-apis-on-users-behalf"
                ]
              },
              {
                "group": "Call Other's APIs with Token Vault",
                "pages": [
                  "intro/token-vault",
                  "intro/connections",
                  "intro/account-linking",
                  "intro/call-others-apis-on-users-behalf"
                ]
              },
              "intro/asynchronous-authorization",
              "intro/authorization-for-rag"
            ]
          },
          {
            "group": "Get Started",
            "pages": [
              "get-started/overview",
              "get-started/user-authentication",
              "get-started/call-your-apis-on-users-behalf",
              "get-started/call-others-apis-on-users-behalf",
              "get-started/asynchronous-authorization",
              "get-started/authorization-for-rag"
            ]
          },
          {
            "group": "Sample Apps",
            "pages": ["sample-apps"]
          },
          {
            "group": "Guides",
            "pages": [
              "guides/google-sign-in-and-auth",
              "guides/client-initiated-account-linking"
            ]
          },
          {
            "group": "Support",
            "pages": ["support"]
          },
          {
            "group": "Glossary",
            "pages": ["glossary"]
          }
        ]
      },
      {
        "tab": "How-Tos",
        "groups": [
          {
            "group": " ",
            "pages": [
              "how-tos/overview",
              "how-tos/check-google-calendar-availability",
              "how-tos/list-github-repositories",
              "how-tos/get-github-issues-python",
              "how-tos/list-slack-channels",
              "how-tos/get-salesforce-opportunities",
              "how-tos/analyze-strava-activities",
              "how-tos/create-spotify-playlist"
            ]
          }
        ]
      },
      {
        "tab": "Auth for MCP",
        "groups": [
          {
            "group": " ",
            "pages": ["mcp/auth-for-mcp", "mcp/cloudflare-mcp"]
          }
        ]
      },
      {
        "tab": "SDKs",
        "groups": [
          {
            "group": " ",
            "pages": [
              "sdks/overview",
              "sdks/javascript-sdk",
              "sdks/python-sdk",
              "sdks/langchain-sdk",
              "sdks/llamaindex-sdk",
              "sdks/vercel-ai-sdk",
              "sdks/cloudflare-sdk",
              "sdks/genkit-sdk"
            ]
          }
        ]
      }
    ]
  },
  "redirects": [
    {
      "source": "/get-started",
      "destination": "/get-started/overview"
    },
    {
      "source": "/user-authentication",
      "destination": "/get-started/user-authentication"
